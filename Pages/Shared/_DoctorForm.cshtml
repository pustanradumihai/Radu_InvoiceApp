@model RadocInvoice.Models.Doctor

<div class="form-group">
    <label for="Name">Name</label>
    <input type="text" class="form-control" id="Name" name="Name" value="@Model.Name" required />
</div>
<div class="form-group">
    <label for="PracticeLicenseNumber">Practice License Nr.</label>
    <input type="text" class="form-control" id="PracticeLicenseNumber" name="PracticeLicenseNumber" value="@Model.PracticeLicenseNumber" required />
</div>
<div class="form-group">
    <label for="Email">Email</label>
    <input type="text" class="form-control" id="Email" name="Email" value="@Model.Email" />
</div>

<div asp-validation-summary="All" class="text-danger"></div>
